{
  "productName": "TauriApp",
  "version": "0.0.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../public"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [{
      "transparent": true,
      "titleBarStyle": "Overlay",
      "decorations": true,
      "hiddenTitle": true,
      "trafficLightPosition": {
        "x": 20,
        "y": 22
      }
    }],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["icons/32x32.png"]
  }
}
