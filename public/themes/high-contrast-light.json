  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "scope": ["keyword", "keyword.control", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#0000ff"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#795e26"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#267f99"
      }
    },
    {
      "scope": ["constant.language", "constant.other"],
      "settings": {
        "foreground": "#0000ff"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#e50000"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#811f3f"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ee0000"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#b5200d"
      }
    },
    {
      "scope": ["keyword.operator.new", "keyword.operator.expression", "keyword.operator.logical", "keyword.operator.delete", "keyword.operator.typeof", "keyword.operator.void", "keyword.operator.instanceof", "keyword.operator.in"],
      "settings": {
        "foreground": "#0000ff"
      }
    },
    {
      "scope": ["meta.definition.variable", "variable.other.constant"],
      "settings": {
        "foreground": "#0070c1"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#267f99"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#0451a5"
      }
    },
    {
      "scope": ["entity.other.attribute-name.class.css", "entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": ["support.type.property-name.css", "support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#e50000"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "scope": ["string.template", "punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#0000ff"
      }
    },
    {
      "scope": ["entity.name.tag.html", "entity.name.tag.xml"],
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading.setext"],
      "settings": {
        "foreground": "#800000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#000080",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
