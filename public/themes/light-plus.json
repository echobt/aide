      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#000080",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
