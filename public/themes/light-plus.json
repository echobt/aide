      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#008000"
      }
    },
    {
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#800000"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "scope": ["entity.name.operator.custom-literal"],
